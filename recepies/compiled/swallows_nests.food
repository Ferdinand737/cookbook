   36	    
   37	    // Prepare coating
   38	    mixing_bowl.add(breadcrumbs)
   39	    mixing_bowl.add(emmental)
   40	    mixing_bowl.mix()
   41	    
   42	    // Coat roulades
   43	    mixing_bowl.beat(raw_eggs)
   44	    mixing_bowl.dip(thin_schnitzels, raw_eggs)
   45	    mixing_bowl.coat(thin_schnitzels)
   46	    
   47	    // Deep fry
   48	    deep_fryer.heat("hot fat")
   49	    deep_fryer.fry(thin_schnitzels, "until golden brown")
   50	    
   51	    return "Swallows' Nests"
   52	}
